{
  "redirects":[
    {
      "source": "/overview/architecture",
      "destination": "/auth-zone/overview/architecture/",
      "permanent":false
    }
  ],
  "rewrites": [
    {
      "source": "/auth-zone/:path*",
      "destination":"/:path*",
      "has": {
        "type": "cookie",
        "key": "auth",
        "value": "testPass"
      }
    },
    {
      "source": "/django/:path*",
      "destination": "https://django.docs.nefino.cc/:path*"
    },
    {
      "source": "/react/:path*",
      "destination": "https://react.docs.nefino.cc/:path*"
    },
    {
      "source": "/airflow/:path*",
      "destination": "https://airflow.docs.nefino.cc/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}